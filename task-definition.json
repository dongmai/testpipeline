{
    "family": "first-run-task-definition",
    "containerDefinitions": [
        {
            "image":"295899798296.dkr.ecr.us-east-1.amazonaws.com/testpipeline:20",
            "name": "nodejs-container",
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
